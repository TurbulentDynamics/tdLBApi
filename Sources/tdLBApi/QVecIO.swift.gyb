%{
codable_types = ['Bool', 'String', 'Double', 'Float',
    'Int', 'Int8', 'Int16', 'Int32', 'Int64',
    'UInt', 'UInt8', 'UInt16', 'UInt32', 'UInt64']
}%


% for type in codable_types:
mutating func encode(_ value: ${type}) throws
% end


